<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cprog"
    android:targetSandboxVersion="2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="29" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@drawable/bc"
        android:label="@string/app_name"
        android:roundIcon="@drawable/bc"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.cprog.Change"
            android:label="Change Song"
            android:parentActivityName="com.example.cprog.MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.example.cprog.Splash"
            android:theme="@style/Theme.MaterialComponents.DayNight.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.cprog.Song"
            android:configChanges="uiMode"
            android:label="Song"
            android:parentActivityName="com.example.cprog.Show"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.example.cprog.Show"
            android:configChanges="uiMode"
            android:label="Songs Lists"
            android:parentActivityName="com.example.cprog.MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.example.cprog.Add"
            android:label="Add Songs"
            android:parentActivityName="com.example.cprog.MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.example.cprog.MainActivity"
            android:configChanges="uiMode"
            android:label="Chord Prog"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
    </application>

</manifest>